<div class='room-layout-table'>

     <div id = "drop-spot"></div>



</div>
<script>
$(function() {
  $( "#drop-spot" ).droppable({

      activeClass: "active-spot",
      hoverClass: "hover-spot",
      drop: function( event, ui ) {
        $('.render-spot').remove();
        //$("#benedict-sidebar-wrapper").append($("#chat-body"));
        //$("#chat-body").animate({left:"+= 50"});
        var children = $('#drop-spot').children();
        if (children.size() > 0) {
                    var video = children[0];
                    document.getElementById(get_free_spot()).appendChild(video);
                    $(video).get(0).play();
        }
        take_spot(5, $("#" + ui.draggable.prop('id')), $("#drop-spot"));
        $("#drop-spot").append( $("#" + ui.draggable.prop('id')));
        $("#" + ui.draggable.prop('id')).css("top", "0%");
        $("#" + ui.draggable.prop('id')).css("left", "0%");
        $("#" + ui.draggable.prop('id')).animate({ width: "100%", height: "100%" });
        $("#" + ui.draggable.prop('id')).get(0).play();


      }
    });
 });


  </script>
